all.javac.release = 11

main {
  hasMacros = true
  implementsMacros = true
  compile {
    modules = [
      optimus.platform.alarms
    ]
    libs = [
      ossjava.logback,
      ossjava.logback.classic,
      ossjava.slf4j,
      ossscala.scala
    ]
    mavenLibs = [
      ch.qos.logback.logback-classic,
      org.slf4j.slf4j-api
    ]
  }
}
